{
    "name": "lbhackney-it/dos-scraper-package-icare-web",
    "type": "library",
    "description": "A Laravel Lumen package for the DoS scraper microservice, to retrieve pages from the Hackney iCare web site.",
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "ext-dom": "*",
        "ext-http": "*",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "ICareWebPageScraper\\": "src/ICareWebPageScraper/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ICareWebPageScraper\\ServiceProvider"
            ]
        }
    }
}